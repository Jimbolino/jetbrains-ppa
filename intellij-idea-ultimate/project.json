{
  "product_code": "IIU",
  "exe_file": "bin/idea.sh",
  "app_name": "IntelliJ IDEA Ultimate",
  "startup_wm_class": "jetbrains-idea",
  "java_export": "IDEA_JDK",

  "package_name": "intellij-idea-ultimate",
  "package_desc": "Web, mobile and enterprise development.",
  "package_desktop_file": [
    "[Desktop Entry]",
    "Name=§{app_name}",
    "Comment=§{package_desc}",
    "Exec=§{package_name} %u",
    "Icon=§{package_name}",
    "Terminal=false",
    "Type=Application",
    "Categories=Development;IDE;",
    "StartupWMClass=§{startup_wm_class}"
  ],
  "package_exe_file": [
    "#!/bin/sh",
    "export §{java_export}=\"$JAVA_HOME\"",
    "exec \"/usr/share/§{package_name}/§{exe_file}\" \"$@\""
  ]
}
