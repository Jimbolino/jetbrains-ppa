# Example dput.cf that defines the host that can be used with dput for uploading.
# Installation: cat dput.cf.example >> /etc/dput.cf
# Upload: dput jetbrains-ppa *.changes

[jetbrains-ppa]
fqdn = ppa.launchpad.net
method = sftp
incoming = ~jonas-groeger/ubuntu/jetbrains/
login = jonas-groeger
allow_unsigned_uploads = 0
